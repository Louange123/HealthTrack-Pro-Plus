<!DOCTYPE html>
<html>
<head>
  <title>Patient Dashboard</title>
</head>
<body>
  <h1>Welcome, <%= patientName %>!</h1>
  <h2>Your Health Data</h2>
  <table>
    <thead>
      <tr>
        <th>Timestamp</th>
        <th>Heart Rate</th>
        <th>Temperature</th>
      </tr>
    </thead>
    <tbody>
      <% patientData.forEach(function(record) { %>
        <tr>
          <td><%= record.timestamp %></td>
          <td><%= record.heartRate %></td>
          <td><%= record.temperature %></td>
        </tr>
      <% }); %>
    </tbody>
  </table>
</body>
</html>
